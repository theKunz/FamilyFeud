<Window x:Class="FamilyFeud.GameWindow"
        x:Name="WindowRoot"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:localControls="clr-namespace:FamilyFeud.Controls"
        mc:Ignorable="d"
        Title="Family Feud" Height="600" Width="800"
        StateChanged="GameWindow_StateChanged"
        SizeChanged="GameWindow_SizeChanged"
        KeyDown="GameWindow_KeyDown"
        ContentRendered="GameWindow_ContentRendered"
        Closing="GameWindow_Closing"
        Icon="pack://application:,,,/FamilyFeud;component/Images/FamilyFeudIcon.png">
  <Grid x:Name="gParentGrid">
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="1920" />
    </Grid.ColumnDefinitions>
    <Grid.RowDefinitions>
      <RowDefinition Height="1080"/>
    </Grid.RowDefinitions>
  </Grid>
</Window>
